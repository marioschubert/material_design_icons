/* MaterialDesignIcons.com */
@import "variables";
@import "functions";

//@import "path";
@font-face {
    font-family: "Material Design Icons";
    src: url("<%= font_path('materialdesignicons-webfont.eot') %>");
    src: url("<%= font_path('materialdesignicons-webfont.eot?#iefix') %>") format("embedded-opentype"),
    url("<%= font_path('materialdesignicons-webfont.woff2') %>") format("woff2"),
    url("<%= font_path('materialdesignicons-webfont.woff') %>") format("woff"),
    url("<%= font_path('materialdesignicons-webfont.ttf') %>") format("truetype");
    font-weight: normal;
    font-style: normal;
}

@import "core";
@import "icons";
@import "extras";
@import "animated";
